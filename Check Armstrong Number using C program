
#include <stdio.h>

int main()
{
    
    int n,r,sum=0;
    printf("ENTER THE NUMBER\n");
    scanf("%d",&n);
    int temp=n;
    while(n>0)
    {
        r=n%10;
        sum+=r*r*r;
        n=n/10;
    }
    
    if(sum==temp)
    printf("ARMSTRONG \n");
    
    else
    printf("NOT A ARMSTRONG\n");
    return 0;
}
